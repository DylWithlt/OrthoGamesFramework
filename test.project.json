{
  "name": "OrthogonalFrameworkV2",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Packages": {
        "$path": "Packages"
      },
      "Vendor": {
        "$path": "Vendor"
      },
      "Assets": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true
      },
      
      "Shared": {
        "$path": "src/Shared"
      },

      "Client": {
        "$path": "src/Client"
      },

      "TestEZ": {
        "$path": "TestEZ"
      }
    },

    "ServerScriptService": {
      "Tests": {
        "$path": "Tests/Server"
      },
      "Server": {
        "$path": "src/Server"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Tests": {
          "$path": "Tests/Client"
        }
      }
    },

    "TestService": {
      "$properties": {"ExecuteWithStudioRun": true, "NumberOfPlayers": 1}
    }
  }
}